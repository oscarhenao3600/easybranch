{
  "name": "easybranch",
  "version": "1.0.0",
  "description": "EasyBranch - Sistema de gesti√≥n de negocios con WhatsApp e IA",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend-admin && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend-admin && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend-admin && npm install",
    "seed": "cd backend && npm run seed",
    "build": "cd backend && npm run build",
    "test": "cd backend && npm test",
    "lint": "cd backend && npm run lint",
    "clean": "rm -rf node_modules backend/node_modules frontend-admin/node_modules",
    "clean:sessions": "rm -rf backend/sessions/*",
    "logs": "cd backend && npm run logs",
    "health": "curl http://localhost:4000/api/health"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "whatsapp",
    "ai",
    "business",
    "management"
  ],
  "author": "EasyBranch Team",
  "license": "MIT"
}
